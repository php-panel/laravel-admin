{
    "name": "php-panel/laravel-admin",
    "description": "laravel admin",
    "type": "library",
    "keywords": [
        "laravel",
        "admin",
        "permission",
        "rbac",
        "access control",
        "grid",
        "form",
        "php-panel"
    ],
    "homepage": "https://github.com/php-panel/laravel-admin",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "leeqvip",
            "email": "leeqvip@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/dom-crawler": "~7.3",
        "laravel/framework": "^12.0",
        "intervention/image": "^3.11"
    },
    "require-dev": {
        "laravel/laravel": ">=12",
        "fakerphp/faker": "~1.24",
        "laravel/browser-kit-testing": "^7.2",
        "spatie/phpunit-watcher": "^1.22.0"
    },
    "autoload": {
        "psr-4": {
            "Ladmin\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Models\\": "tests/models",
            "Tests\\Controllers\\": "tests/controllers"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "XDEBUG_MODE=coverage ./vendor/bin/phpunit"
    },
    "suggest": {
        "intervention/image": "Required to handling and manipulation upload images (~2.3).",
        "spatie/eloquent-sortable": "Required to built orderable gird."
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ladmin\\AdminServiceProvider"
            ],
            "aliases": {
                "Admin": "Ladmin\\Facades\\Admin"
            }
        }
    }
}
